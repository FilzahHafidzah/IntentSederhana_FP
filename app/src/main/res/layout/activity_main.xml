<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:paddingTop="@dimen/vertical_margin"
    android:paddingBottom="@dimen/vertical_margin"
    android:paddingRight="@dimen/horizontal_margin"
    android:paddingLeft="@dimen/horizontal_margin"
    android:background="@color/colorPutih">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/logo_hitam"
        android:paddingBottom="@dimen/vertical_margin"
        android:paddingLeft="@dimen/horizontal_margin"
        android:paddingRight="@dimen/horizontal_margin"
        android:paddingTop="@dimen/vertical_margin"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center">

        <ImageView
            android:id="@+id/tn_go_mart"
            android:layout_width="@dimen/vertical_size"
            android:layout_height="@dimen/horizontal_size"
            android:src="@drawable/menu_gomart"
            android:paddingRight="@dimen/horizontal_margin"/>

        <ImageView
            android:id="@+id/tn_go_ride"
            android:layout_width="@dimen/vertical_size"
            android:layout_height="@dimen/horizontal_size"
            android:src="@drawable/menu_goride"
            android:paddingRight="@dimen/horizontal_margin"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center">

        <ImageView
            android:id="@+id/tn_go_food"
            android:layout_width="@dimen/vertical_size"
            android:layout_height="@dimen/horizontal_size"
            android:src="@drawable/menu_gofood"
            android:paddingRight="@dimen/horizontal_margin"/>

        <ImageView
            android:id="@+id/tn_go_send"
            android:layout_width="@dimen/vertical_size"
            android:layout_height="@dimen/horizontal_size"
            android:src="@drawable/menu_gosend"
            android:paddingRight="@dimen/horizontal_margin"/>


    </LinearLayout>


</LinearLayout>